  "private": true,
  "workspaces": ["packages/ws-server", "packages/game-vue", "packages/shared"],
  "scripts": {
    "dev:vue": "npm run dev --workspace=game-vue",
    "dev:react": "npm run dev --workspace=game-react",
    "dev:server": "npm run dev --workspace=ws-server",
